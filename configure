               sed 's/\([0-9]*\)\.\([0-9]*\)\.\([0-9]*\)/\1/'`
    req_minor=`echo $min_libgcrypt_version | \
               sed 's/\([0-9]*\)\.\([0-9]*\)\.\([0-9]*\)/\2/'`
    req_micro=`echo $min_libgcrypt_version | \
               sed 's/\([0-9]*\)\.\([0-9]*\)\.\([0-9]*\)/\3/'`
    libgcrypt_config_version=`$LIBGCRYPT_CONFIG --version`
